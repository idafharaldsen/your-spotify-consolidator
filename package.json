{
  "name": "your-spotify-consolidator",
  "version": "1.0.0",
  "description": "A tool to fetch and consolidate Spotify top albums data",
  "main": "index.js",
  "scripts": {
    "fetch-albums": "tsx scripts/fetch-api/fetch-top-albums.ts",
    "clean-albums": "tsx scripts/api-data/clean-top-albums.ts",
    "clean-albums-interactive": "tsx scripts/api-data/clean-top-albums-interactive.ts",
    "fetch-songs": "tsx scripts/fetch-api/fetch-top-songs.ts",
    "clean-songs": "tsx scripts/api-data/clean-top-songs.ts",
    "fetch-artists": "tsx scripts/fetch-api/fetch-top-artists.ts",
    "clean-artists": "tsx scripts/api-data/clean-top-artists.ts",
    "setup-ai": "tsx scripts/setup-ai-agent.ts",
    "ai-consolidate": "tsx scripts/ai-consolidate-albums.ts",
    "consolidate-all": "tsx scripts/mongo/consolidate-all-mongodb-data.ts",
    "consolidate-albums-with-songs": "tsx scripts/mongo/consolidate-albums-with-songs.ts",
    "fetch-recent-plays": "tsx scripts/fetch-recent-plays.ts",
    "merge-recent-data": "tsx scripts/merge-recent-data.ts",
    "build-complete-history": "tsx scripts/mongo/build-complete-history.ts",
    "setup-spotify-auth": "tsx scripts/setup-spotify-auth.ts",
    "generate-cleaned-files": "tsx scripts/mongo/generate-cleaned-files-from-history.ts",
    "merge-streaming-history": "tsx scripts/merge-streaming-history.ts",
    "enrich-streaming-history": "tsx scripts/enrich-streaming-history.ts",
    "test-spotify-api": "tsx scripts/test-spotify-api.ts",
    "convert-history-to-top-data": "tsx scripts/api-data/convert-history-to-top-data.ts",
    "clean-all-top-data": "npm run clean-songs && npm run clean-albums && npm run clean-artists",
    "process-daily-data": "npm run convert-history-to-top-data && npm run clean-all-top-data",
    "web:dev": "cd web-app && npm run dev",
    "web:build": "cd web-app && npm run build",
    "web:start": "cd web-app && npm run start"
  },
  "keywords": [
    "spotify",
    "music",
    "data",
    "consolidation",
    "api"
  ],
  "author": "Your Name",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.0.0",
    "tsx": "^4.0.0",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@types/cli-progress": "^3.11.6",
    "@types/commander": "^2.12.0",
    "axios": "^1.12.2",
    "cli-progress": "^3.12.0",
    "commander": "^11.1.0",
    "dotenv": "^16.0.0",
    "glob": "^10.3.10"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "directories": {
    "doc": "docs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tordar/your-spotify-consolidator.git"
  },
  "type": "commonjs",
  "bugs": {
    "url": "https://github.com/tordar/your-spotify-consolidator/issues"
  },
  "homepage": "https://github.com/tordar/your-spotify-consolidator#readme"
}
